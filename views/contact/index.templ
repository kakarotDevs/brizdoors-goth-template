package contact

import "github.com/kakarotDevs/brizdoors-goth-template/layouts"

templ Index(isDarkMode bool) {
	@layouts.Base("Contact", isDarkMode) {
		<div class="p-8">
			<h1 class="text-3xl font-bold text-text">Contact Us</h1>
			<p class="mt-2 text-text-muted">Send us a message or drop by. We're always ready to help.</p>
		</div>
	}
}
